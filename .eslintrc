{
  "extends": "eslint-config-airbnb",
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "comma-dangle": [0, "always-multiline"],
    "func-names": 0,
    "no-multi-spaces": 0,
    "strict": 0,
    "new-cap": 0,
    "padded-blocks": 0,
    "no-unused-vars": 0,
    "no-unused-expressions": 0,
    "no-console": 0
  }
}
